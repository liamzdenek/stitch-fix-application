{
  "name": "email-processor-go",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "packages/email-processor-go/src",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "cd packages/email-processor-go && mkdir -p dist && go build -o dist/email-processor ./src/main.go"
        ],
        "parallel": false
      },
      "outputs": ["{workspaceRoot}/packages/email-processor-go/dist"]
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "cd packages/email-processor-go && go vet ./..."
        ],
        "parallel": false
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "cd packages/email-processor-go && go test ./..."
        ],
        "parallel": false
      }
    }
  },
  "tags": []
}